{
    "name": "statistic",
    "displayName": "statistic",
    "description": "%description%",
    "preview": true,
    "version": "0.3.0",
    "publisher": "caixw",
    "author": {
        "name": "caixw",
        "url": "https://github.com/caixw"
    },
    "keywords": [
        "statistic",
        "total",
        "line",
        "counter",
        "analytics",
        "project"
    ],
    "license": "MIT",
    "homepage": "https://github.com/caixw/vscode-statistic",
    "bugs": {
        "url": "https://github.com/caixw/vscode-statistic/issues",
        "email": "xnotepad@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/caixw/vscode-statistic.git"
    },
    "engines": {
        "vscode": "^1.37.0"
    },
    "galleryBanner": {
        "color": "#5c2d91",
        "theme": "dark"
    },
    "icon": "images/icon.png",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:caixw.statistic.show"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "caixw.statistic.show",
                "title": "%caixw.statistic.show.title%",
                "category": "statistic",
                "icon": {
                    "dark": "images/icon.svg",
                    "light": "images/icon.svg"
                }
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "caixw.statistic.show",
                    "when": "workbenchState=='workspace' && explorerResourceIsRoot || workbenchState=='folder'"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.13",
        "@types/glob": "^7.1.1",
        "@types/mime-types": "^2.1.0",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.37.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "vscode-test": "^1.0.2"
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.3",
        "ignore": "^5.1.2",
        "mime-types": "^2.1.24"
    }
}
